---
export interface Props {
  data: [string, string][]
}

const { data } = Astro.props
---

<table class="text-sm">
  {
    data.map(([key, value]) => (
      <tr>
        <td class="font-medium">{key}</td>
        <td class="pl-4">
          <p>{value}</p>
        </td>
      </tr>
    ))
  }
</table>
