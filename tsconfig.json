{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "baseUrl": ".",
    "paths": {
      "$components/*": ["src/components/*"],
      "$constants/*": ["src/constants/*"],
      "$layouts/*": ["src/layouts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
